        <body>
            <div class ="container">
                <div class ="row">
                    <div class ="col-12 text-center">
                        <h2>Register Patient</h2>
                    </div>
                </div><br>
                <div class ="row">
                    <div class ="col-md-3 col-xs-12"></div>
                    <div class ="col-md-6 col-xs-12">
                        <form action ="/case/register"method ="POST">
                            <div class ="form-group"><input type ="text" name ="patient"class ="form-control" required placeholder="Name"></div>
                            <div class ="form-group"><input type ="text" name ="age"class ="form-control" required placeholder="Age"></div>
                            <div class ="form-group"><input type ="date" name ="DOA"class ="form-control" required placeholder="Date of Birth"></div>
                            <div class ="form-group"><input type ="text" name ="disease[0]"class ="form-control" placeholder="Disease 1"></div>
                            <div class ="form-group"><input type ="text" name ="disease[1]"class ="form-control" placeholder="Disease 2"disabled></div>
                            <div class ="form-group"><input type ="text" name ="disease[2]"class ="form-control" placeholder="Disease 3"disabled></div>
                            <div class ="form-group"><input type ="text" name ="disease[3]"class ="form-control" placeholder="Disease 4"disabled></div>
                            <div class ="form-group">
                                <input type ="radio" name ="Effect" value ="minor">&nbsp;&nbsp;&nbsp;Minor&nbsp;&nbsp;&nbsp;&nbsp;
                                <input type ="radio" name ="Effect" value ="major">&nbsp;&nbsp;&nbsp;Major&nbsp;&nbsp;&nbsp;&nbsp;
                                <input type ="radio" name ="Effect" value ="fatal">&nbsp;&nbsp;&nbsp;Fatal&nbsp;&nbsp;&nbsp;&nbsp;
                                <input type ="radio" name ="Effect" value ="brutal">&nbsp;&nbsp;&nbsp;Brutal&nbsp;&nbsp;&nbsp;&nbsp;
                            </div>
                                
                            <div class ="form-group"><input type ="number" name ="pincode"class ="form-control" placeholder="Pincode"></div>
                            <button class ="btn btn-success" name= "submit" type ="submit">Submit</button>
                            </div>
                        </form>
                    </div>
                </div>
            </div>

</body>
</html>
